<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="KGMusicProvider" class="cn.zjm404.demo.di.template.KGMusicProvider"/>
    <bean id="WYYMusicProvider" class="cn.zjm404.demo.di.template.WYYMusicProvider"/>
    <bean id="TTPODMusicProvider" class="cn.zjm404.demo.di.template.TTPODMusicProvider"/>
    <bean id="MusicProviderFacade"  class="cn.zjm404.demo.di.template.MusicProviderFacade"
          lazy-init="true" scope="singleton">
        <property name="musicProvidersList">
            <list>
                <ref bean="KGMusicProvider"/>
                <ref bean="TTPODMusicProvider"/>
                <ref bean="WYYMusicProvider"/>
            </list>
        </property>
    </bean>
</beans>